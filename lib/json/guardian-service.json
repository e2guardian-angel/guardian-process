{
  "apiVersion": "v1",
  "kind": "Service",
  "metadata": {
    "name": "guardian-angel",
    "labels": {
      "app": "guardian-angel"
    }
  },
  "spec": {
    "type": "NodePort",
    "selector": {
      "app": "guardian-angel"
    },
    "ports": [
      {
        "name": "http",
        "protocol": "TCP",
        "port": 80,
        "targetPort": 80,
        "nodePort": 30080
      }
    ]
  }
}